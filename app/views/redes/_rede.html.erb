<div id="<%= dom_id rede %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Nome:</strong>
    <%= rede.nome %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Token:</strong>
    <%= rede.token %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Abrir", rede, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Editar', edit_rede_path(rede), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
